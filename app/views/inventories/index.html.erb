<h1>Inventories</h1>
<%= link_to '+ New Inventory', new_inventory_path %>
<table>
  <thead>
    <tr>
      <th>Product</th>
      <th>Quantity</th>
    </tr>
  </thead>
  <tbody>
    <% @inventories.each do |inventory| %>
    <tr>
      <td><%= inventory.product %></td>
      <td><%= inventory.quantity %></td>
    </tr>
    <% end %>
  </tbody>
</table>
